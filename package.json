{
	"name": "uoc-lv",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "env-cmd -f './.env.development' node ./connections/tables/index.js && next dev -p 3000 | pino-pretty",
		"build-local": "env-cmd -f './.env.local' node ./connections/tables/index.js && next build",
		"build": "next build",
		"start": "node ./connections/tables/index.js && next start -p 80",
		"start-dev": "env-cmd -f './.env.development' rm -rf .next && next build && next start -p 3000",
		"lint": "next lint",
		"cdn-dev": "env-cmd -f './.env.development' nodemon ./cdn.js",
		"cdn": "env-cmd -f './.env.production' node ./cdn.js",
		"xml": "env-cmd -f './.env.production' node ./xml.js",
		"analyze": "ANALYZE=true next build"
	},
	"dependencies": {
		"@mantine/core": "4.2.9",
		"@mantine/dropzone": "4.2.9",
		"@mantine/form": "4.2.9",
		"@mantine/next": "4.2.9",
		"@mantine/rte": "4.2.9",
		"busboy": "1.6.0",
		"env-cmd": "10.1.0",
		"express": "4.18.1",
		"format-schema": "3.0.0",
		"iron-session": "6.1.3",
		"next": "12.1.6",
		"next-seo": "5.4.0",
		"pg": "8.7.3",
		"pino": "7.11.0",
		"pino-pretty": "7.6.1",
		"pino-tee": "0.3.0",
		"plaiceholder": "2.4.0",
		"react": "18.1.0",
		"react-dom": "18.1.0",
		"react-h5-audio-player": "3.8.4",
		"rss-parser": "3.12.0",
		"scpp": "1.0.5",
		"sharp": "0.30.6",
		"sitemap": "7.1.1",
		"swr": "1.3.0",
		"tabler-icons-react": "1.48.0"
	},
	"devDependencies": {
		"@next/bundle-analyzer": "12.2.0",
		"eslint": "8.17.0",
		"eslint-config-next": "12.1.6",
		"eslint-plugin-unused-imports": "2.0.0",
		"prettier": "2.7.0"
	}
}
